<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    
                    xmlns:view="clr-namespace:IGA_GUI_Module.View"
                    
                    xmlns:viewModel="clr-namespace:IGA_GUI_Module.ViewModel"
                    >
    
    <DataTemplate DataType="{x:Type viewModel:MainViewModel}">
        <view:MainView></view:MainView>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type viewModel:DataAnalysisViewModel}">
        <view:DataAnalysisView></view:DataAnalysisView>
    </DataTemplate>
    
</ResourceDictionary>